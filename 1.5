import java.util.Scanner;

public class P {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        boolean p = n > 1;
        for (int i = 2; i * i <= n; i++)
            if (n % i == 0) p = false;
        System.out.print(p ? "Yes" : "No");
    }
}
