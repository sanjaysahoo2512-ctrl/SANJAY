import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt(), sum = 0, arr[] = new int[n];

        for (int i = 0; i < n; i++)
            sum += (arr[i] = sc.nextInt());

        for (int i = n - 1; i >= 0; i--)
            System.out.print(arr[i] + " ");

        System.out.println("\nSum: " + sum);
    }
}
